{{- $.Scratch.Add "index" slice -}}
{{- range where .Site.RegularPages "Type" "not in" (slice "page") -}}
  {{- $.Scratch.Add "index" (dict "title" .Title "permalink" .Permalink "description" .Description "content" .Plain "date" (.Date.Format "2006-01-02")) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}